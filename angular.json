{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "enigma": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/enigma",
      "sourceRoot": "apps/enigma/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/enigma",
            "index": "apps/enigma/src/index.html",
            "main": "apps/enigma/src/main.ts",
            "polyfills": "apps/enigma/src/polyfills.ts",
            "tsConfig": "apps/enigma/tsconfig.app.json",
            "aot": false,
            "assets": ["apps/enigma/src/favicon.ico", "apps/enigma/src/assets"],
            "styles": ["apps/enigma/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/enigma/src/environments/environment.ts",
                  "with": "apps/enigma/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "enigma:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "enigma:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "enigma:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/enigma/tsconfig.app.json",
              "apps/enigma/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/enigma/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/enigma/jest.config.js",
            "tsConfig": "apps/enigma/tsconfig.spec.json",
            "setupFile": "apps/enigma/src/test-setup.ts"
          }
        }
      }
    },
    "enigma-e2e": {
      "root": "apps/enigma-e2e",
      "sourceRoot": "apps/enigma-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/enigma-e2e/cypress.json",
            "tsConfig": "apps/enigma-e2e/tsconfig.e2e.json",
            "devServerTarget": "enigma:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "enigma:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/enigma-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**", "!apps/enigma-e2e/**"]
          }
        }
      }
    },
    "enigma-machine": {
      "projectType": "library",
      "root": "libs/enigma-machine",
      "sourceRoot": "libs/enigma-machine/src",
      "prefix": "enigma",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/enigma-machine/tsconfig.lib.json",
              "libs/enigma-machine/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/enigma-machine/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/enigma-machine/jest.config.js",
            "tsConfig": "libs/enigma-machine/tsconfig.spec.json",
            "setupFile": "libs/enigma-machine/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "enigma"
}
